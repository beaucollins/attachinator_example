<h2>New Album</h2>

<h1>New piece</h1>

<% form_for @album, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
      <%- f.attachment_fields_for :photos do |attachment_fields| %>
            <%= attachment_fields.file_field :photo %>
          <%- end -%>

</p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', albums_path %>

